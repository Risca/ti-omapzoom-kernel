config VIDEO_OMAP2_VOUT
	tristate "OMAP2+ V4L2-Display driver"
	depends on ARCH_OMAP2PLUS
	select VIDEOBUF_GEN
	select VIDEOBUF_DMA_CONTIG
	select OMAP2_DSS
	select OMAP2_VRAM
	select OMAP2_VRFB if ARCH_OMAP2 || ARCH_OMAP3
	default n
	---help---
	  V4L2 Display driver support for OMAP2+ based boards.
